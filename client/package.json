{
  "name": "inab",
  "version": "0.0.1",
  "description": "INAB",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "remove-dist": "rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",

    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",

    "test": "mocha tools/testSetup.js \"./test/*.js\" --reporter progress --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nauktis/inab.git"
  },
  "author": "Nauktis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nauktis/inab/issues"
  },
  "homepage": "https://github.com/Nauktis/inab#readme",
  "dependencies": {
    "axios": "^0.12.0",
    "cuid": "^1.3.8",
    "font-awesome": "^4.6.3",
    "lodash": "^4.16.1",
    "moment": "^2.15.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-datepicker": "^0.29.0",
    "react-dom": "^15.3.2",
    "react-fontawesome": "^1.2.0",
    "react-redux": "^4.4.5",
    "react-redux-modal": "^0.4.3",
    "react-selectize": "^2.0.3",
    "redux": "^3.6.0",
    "redux-crud": "^1.0.0",
    "redux-form": "^6.0.5",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "redux-ui": "0.0.15",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-display-name": "^2.0.0",
