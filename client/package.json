    "bootstrap": "^4.0.0-beta",
    "cuid": "^1.3.8",
    "font-awesome": "^4.7.0",
    "hw-react-shared": "hwaterke/react-shared#build",
    "immutable": "^3.8.1",
    "inab-shared": "hwaterke/inab-shared#build",
    "material-ui": "^0.18.0",
    "moment": "^2.17.1",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-datepicker": "^0.53.0",
    "react-dom": "^15.6.1",
    "react-dom-factories": "^1.0.1",
